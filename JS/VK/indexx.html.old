            </div>
            <div id="addTrackWall" title="Отправить трек на стену другу">
            </div>
            <div id="random" title="Включение/отключение случайного воспроизведения">
            </div>
            <div id="repeat" title="Включение/отключение повторного воспроизведения трека">
            </div>
        </div>
        <div id="volume" title="Громкость">
            <!--<div id="valueVolume">
            </div>-->
            <div id="slider">
            </div>
        </div>
        <div id="wrapPlayer">
            <div id="player">
            </div>
        </div>
    </div>
    <div style="position: absolute; right: 0px; top: 0px; text-align: right;" id="logo">
